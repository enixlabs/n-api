  "scripts": {
    "start:all": "",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "----| [ DOCS ] |----": "",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s -o",
    "docosaurus": "cd docs && npx docusaurus start",
    "----| [ BUILD ] |----": "",
    "build:api": "nest build api && npm run copy:assets",
    "build:discord": "nest build discord",
    "build:openai": "nest build openai",
    "build:swc": "npx swc --out-dir dist -w src",
    "----| [ DEV ] |----": "",
    "start:dev:api": "nest start api --watch",
    "start:dev:api:webpack": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev:discord": "nest start discord --watch",
    "start:dev:openai": "nest start openai --watch",
    "start:repl": "nest start --entryFile repl",
    "----| [ START ] |----": "",
    "start:api": "nest start",
    "start:swc": "nodemon dist/main",
    "----| [ START DEBUG ] |----": "",
    "start:debug:api": "nest start --debug --watch",
    "----| [ START PROD ] |----": "",
    "start:prod:api": "node dist/main",
    "----| [ TEST ] |----": "",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "copy:assets": "cpx ./apps/api/web/**/* ./dist/apps/web/"
  },
