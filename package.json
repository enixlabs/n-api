  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@configuration/configuration(|/.*)$": "<rootDir>/libs/configuration/src/$1",
      "^@firebase/firebase(|/.*)$": "<rootDir>/libs/firebase/src/$1",
      "^@database/database(|/.*)$": "<rootDir>/libs/database/src/$1"
    }
  }
